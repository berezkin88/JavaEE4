<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName"
>

    <bean id="productDao" class="crud.dao.ProductDaoImpl"/>
    <bean id="productService" class="crud.service.ProductServiceImpl"/>
    <bean id="userDao" class="crud.dao.UserDaoImpl"/>
    <bean id="securityService" class="crud.service.SecurityServiceImpl"/>
    <bean id="userService" class="crud.service.UserServiceImpl"/>
    <bean id="userDetailService" class="crud.service.UserDetailServiceImpl"/>

</beans>